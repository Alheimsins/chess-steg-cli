{
  "name": "chess-steg-cli",
  "version": "1.2.0",
  "description": "Client for chess steganography - Hiding messages in chess games",
  "main": "index.js",
  "author": "",
  "scripts": {
    "test": "standard && npm audit && ava",
    "standard-fix": "standard --fix",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "standard": {
    "ignore": [
      "src/*"
    ]
  },
  "files": [
    "src/*",
    "bin/*"
  ],
  "bin": {
    "chess-steg": "./bin/chess-steg"
  },
  "keywords": [
    "chess",
    "steganography"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alheimsins/chess-steg-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Alheimsins/chess-steg-cli/issues"
  },
  "license": "MIT",
  "dependencies": {
    "args": "5.0.0",
    "axios": "0.18.0",
    "big-integer": "1.6.40",
    "chess.js": "0.10.2",
    "clipboardy": "1.2.3",
    "opn": "5.4.0"
  },
  "devDependencies": {
    "ava": "1.1.0",
    "standard": "12.0.1"
  }
}
